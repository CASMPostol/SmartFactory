								<xs:annotation>
									<xs:documentation source="doc">Miejsce i data.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="Miejsce" use="required">
										<xs:annotation>
											<xs:documentation source="doc">Miejsce wypełnienia zgłoszenia.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="35"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="Data" type="xs:date" use="required">
										<xs:annotation>
											<xs:documentation source="doc">Data wypełnienia zgłoszenia.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="NazwiskoImie" use="required">
										<xs:annotation>
											<xs:documentation source="doc">Nazwisko i imię.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="35"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="Stanowisko" use="optional">
										<xs:annotation>
											<xs:documentation source="doc">Stanowisko.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="35"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="Telefon" use="required">
										<xs:annotation>
											<xs:documentation source="doc">Telefon kontaktowy.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="15"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="WynikKontroliGr">
								<xs:annotation>
									<xs:documentation source="doc">Wynik kontroli w urzędzie wyprowadzenia.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="Kod" use="required">
										<xs:annotation>
											<xs:documentation source="doc">Kod wyniku kontroli.</xs:documentation>
											<xs:documentation source="list">050</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="StanZamkniec" use="optional">
										<xs:annotation>
											<xs:documentation source="doc">Stan zamknięć celnych przy przedstawieniu towaru w urzędzie wyprowadzenia (jeśli zamknięcia były założone w urzędzie wywozu).  "0" - NOK, "1" - OK.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:boolean">
												<xs:pattern value="0|1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="GrK" minOccurs="0" maxOccurs="9">
								<xs:annotation>
									<xs:documentation source="doc">Komentarz urzędu wyprowadzenia do zgłoszenia.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="Tekst" use="required">
										<xs:annotation>
											<xs:documentation source="doc">Tekst komentarza.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="140"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="NrWlasny" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Numer własny zgłoszenia (lub ostatniego zaakceptowanego sprostowania, jeśli miało miejsce).</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="22"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="MRN" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Numer ewidencyjny zgłoszenia.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="18"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="UCWywozu" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Kod urzędu celnego wywozu (urzędu złożenia zgłoszenia).</xs:documentation>
								<xs:documentation source="list">001</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="8"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="UCWyprowadzenia" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Kod deklarowanego urzędu celnego wyprowadzenia.</xs:documentation>
								<xs:documentation source="list">001</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="8"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="UCWyprowadzeniaZmienionyNa" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Kod faktycznego urzędu celnego wyprowadzenia, jeśli był inny niż deklarowany.</xs:documentation>
								<xs:documentation source="list">001</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="8"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="P1a" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Deklaracja.</xs:documentation>
								<xs:documentation source="list">016</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="P1b" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Deklaracja. Wartość "Z" oznacza, że zgłoszenie jest jednocześnie powiadomieniem.</xs:documentation>
								<xs:documentation source="list">017</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="LiczbaPozycji" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Liczba pozycji towarowych.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="3"/>
									<xs:fractionDigits value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="LiczbaPozycjiGr" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Liczba pozycji w urzędzie wyprowadzenia, jeśli była inna niż w urzędzie wywozu.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="3"/>
									<xs:fractionDigits value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="LiczbaPozycjiGrK" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Komentarz urzędu wyprowadzenia do liczby pozycji.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="140"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="LiczbaOpakowan" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Całkowita liczba opakowań.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="7"/>
									<xs:fractionDigits value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="LiczbaOpakowanGr" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Całkowita liczba opakowań w urzędzie wyprowadzenia, jeśli była inna niż w urzędzie wywozu.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="7"/>
									<xs:fractionDigits value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="LiczbaOpakowanGrK" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Komentarz urzędu wyprowadzenia do całkowitej liczby opakowań.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="140"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="CRN" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Unikalny, przypisany do danej przesyłki numer nadany towarom (Commercial Reference Number). Zalecane jest użycie kodu o strukturze przewidzianej przez WCO (ISO15459) lub jego odpowiednika.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="70"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="KrajWysylki" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Kod kraju wysyłki/eksportu.</xs:documentation>
								<xs:documentation source="list">007</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="KrajPrzeznaczenia" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Kod kraju przeznaczenia.</xs:documentation>
								<xs:documentation source="list">007</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Kontenery" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Informacja o występowaniu kontenerów przy przekraczaniu granicy wspólnoty. "0" - nie występują, "1" - występują.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:boolean">
									<xs:pattern value="0|1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="MasaBrutto" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Całkowita masa brutto.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="15"/>
									<xs:fractionDigits value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="MasaBruttoGr" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Całkowita masa brutto w urzędzie wyprowadzenia, jeśli była inna niż w urzędzie wywozu.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="15"/>
									<xs:fractionDigits value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="MasaBruttoGrK" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Komentarz urzędu wyprowadzenia do całkowitej masy brutto.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="140"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="SumaOplat" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Całkowita suma opłat pozycji towarowych.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="15"/>
									<xs:fractionDigits value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="SpecyficzneOkolicznosci" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Kod specyficznych okoliczności tzn. wyłączeń z podstawowego zakresu danych.</xs:documentation>
								<xs:documentation source="list">215</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="OplatyTransportoweMP" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Kod metody płatności opłat transportowych.</xs:documentation>
								<xs:documentation source="list">300</xs:documentation>
								<xs:documentation source="rule">-</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:length value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Skladowanie" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Informacja określająca, czy w przypadku zgłoszenia dokonywanego bezpośrednio w urzędzie wyprowadzenia (urząd wywozu jest tożsamy z urzędem wyprowadzenia) towar będzie wyprowadzany na podstawie manifestu(ów) eksportowych. "0" - Nie, "1" - Tak.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:boolean">
									<xs:pattern value="0|1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="DataPrzyjecia" type="xs:dateTime" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Data i czas przyjęcia zgłoszenia (tj. nadania numeru ewidencyjnego).</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="DataZwolnienia" type="xs:dateTime" use="required">
							<xs:annotation>
								<xs:documentation source="doc">Data zwolnienia zgłoszenia do procedury wywozu.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="DataWyprowadzenia" type="xs:date" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Data wyprowadzenia całości towaru zgłoszenia poza granicę UE.</xs:documentation>
								<xs:documentation source="rule">RW1</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="DataZatrzymania" type="xs:date" use="optional">
							<xs:annotation>
								<xs:documentation source="doc">Data zatrzymania towaru w urzędzie wyprowadzenia.</xs:documentation>
								<xs:documentation source="rule">RW1</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="DokumentyAlternatywne" type="xs:boolean" use="optional">
							<xs:annotation>
								<xs:documentation>Informacja określająca, czy potweirdzenie wyprowadzenia wydano na podstawie informacji z Urzędu Wyprowadzenia czy też na podstawie innych dokumentów. "0" - Nie, "1" - Tak.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element ref="ds:Signature" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="NrWlasny" use="required">
				<xs:annotation>
					<xs:documentation source="doc">Numer własny tego komunikatu.</xs:documentation>
					<xs:documentation source="rule">G21</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="22"/>
						<xs:pattern value="[A-Z0-9\-#][A-Z0-9\-]{0,21}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="EmailPodmiotu" use="optional">
				<xs:annotation>
					<xs:documentation source="doc">Adres email, na który jest przesyłany ten komunikat (tylko jeśli był podany w zgłoszeniu).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PlacowkaPodmiotu" use="optional">
				<xs:annotation>
					<xs:documentation source="doc">Identyfikator placówki podmiotu. Atrybut używany jest do identyfikacji placówki podmiotu przy wysyłaniu i odbieraniu komunikatów poprzez interfejs niewizualny.</xs:documentation>
					<xs:documentation source="rule">G26</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
						<xs:pattern value="[A-Z0-9]{2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>

    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:CAS.SmartFactory.CW.Dashboards.GenerateSadConsignmentWebPart" 
    x:Class="CAS.SmartFactory.CW.Dashboards.GenerateSadConsignmentWebPart.MainPage"

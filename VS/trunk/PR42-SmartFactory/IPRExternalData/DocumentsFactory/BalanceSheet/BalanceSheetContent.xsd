      <xs:element minOccurs="1" maxOccurs="1" name="TotalTobaccoInCigarettesProduction" type="xs:double" />
      <xs:element minOccurs="1" maxOccurs="1" name="TotalTobaccoInCutfillerWarehouse" type="xs:double" />
      <xs:element minOccurs="1" maxOccurs="1" name="TotalBalance" type="xs:double" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ArrayOfIPRRow">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="IPRRow" nillable="true" type="tns:IPRRow" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="IPRRow">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="EntryDocumentNo" type="xs:string" />

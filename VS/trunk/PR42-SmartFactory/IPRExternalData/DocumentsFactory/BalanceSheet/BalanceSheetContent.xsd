      <xs:element minOccurs="1" maxOccurs="1" name="TotalSHWasteOveruseCSNotStarted" type="xs:double" />
      <xs:element minOccurs="1" maxOccurs="1" name="TotalDustCSNotStarted" type="xs:double" />
      <xs:element minOccurs="1" maxOccurs="1" name="TotalTobaccoAvailable" type="xs:double" />
      <xs:element minOccurs="1" maxOccurs="1" name="TotalTobaccoInWarehouse" type="xs:double" />

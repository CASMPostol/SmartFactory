      <xs:element minOccurs="1" maxOccurs="1" name="TobaccoInCutfillerWarehouse" type="xs:double" />
      <xs:element minOccurs="1" maxOccurs="1" name="Balance" type="xs:double" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="JSOContent">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Disposals" type="tns:ArrayOfDisposalRow" />
      <xs:element minOccurs="1" maxOccurs="1" name="PreviousMonthDate" type="xs:date" />
      <xs:element minOccurs="1" maxOccurs="1" name="PreviousMonthQuantity" type="xs:double" />
